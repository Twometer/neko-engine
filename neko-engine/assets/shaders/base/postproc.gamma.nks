#include postproc.base.nks

#begin fragment
    out vec4 color;

    in vec2 texCoords;

    uniform sampler2D textureSampler;

    void main() {
        color = texture(textureSampler, texCoords);

        float gamma = 1.8;
        color.rgb = pow(color.rgb, vec3(1.0 / gamma));
    }
#end fragment